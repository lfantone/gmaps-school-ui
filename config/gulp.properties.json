{
  "path": {
    "bundle": "app/bundle",
    "dest": "app/",
    "entries": "app/scripts/app.js"
  },
  "webserver": {
    "directoryListing": false,
    "fallback": "index.html",
    "livereload": true,
    "open": "http://localhost:9001/",
    "port": 9001,
    "proxies": [
      {"source": "/api", "target": "http://www.g-obra.com/"},
      {"source": "/web/js/", "target": "http://www.g-obra.com/web/js"}
    ]
  }
}
